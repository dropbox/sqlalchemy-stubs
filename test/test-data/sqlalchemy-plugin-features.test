    __tablename__ = 'users'
    id = Column(Integer(), primary_key=True)
    first_other = relationship('Other', uselist=False)
    second_other = relationship('Other', uselist=True)
    first_bad_other = relationship('Other')  # E: Cannot figure out kind of relationship \
